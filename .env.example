# Variables d'environnement pour la production
# Copiez ce fichier vers .env et modifiez les valeurs

# Base de données PostgreSQL
POSTGRES_DB=eshop
POSTGRES_USER=eshop_user
POSTGRES_PASSWORD=your_secure_password_here

# Chaîne de connexion complète pour l'API
DATABASE_CONNECTION_STRING=Host=postgres;Database=eshop;Username=eshop_user;Password=your_secure_password_here

# JWT Configuration (si nécessaire)
JWT_SECRET=your_jwt_secret_key_here
JWT_ISSUER=https://yourapi.example.com
JWT_AUDIENCE=eshop-api

# Google OAuth (si utilisé)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret